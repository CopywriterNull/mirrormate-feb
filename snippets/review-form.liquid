{% if initial %}
    {% assign line = 0 %}
{% else %}
    {% assign line = "{{ line }}" %}
{% endif %}

<div class="main__block" js-review-form data-block-number="">
    <div class="block__count" js-block-count></div>
    <div class="block__contents">
        <div class="column__grid">
            <div class="block__column">
                <div class="product__contents">
                    <div class="product__img block__icon">
                        <img src="" js-swatch-img>
                    </div>
                    <div class="product__meta">
                        <h4 class="column__title hide__mobile">frame style</h4>
                        <h4 class="column__title hide__desktop" js-product-title></h4>
                        <p class="column__rte hide__mobile" js-product-title></p>
                        <div class="details__grid hide__desktop info__grid">
                            <div class="details">
                                <span class="field_label">Qty:</span>
                                <span class="field-value" js-quantity-field></span>
                            </div>
                            <div class="details">
                                <span class="field_label">Room/Unit #:</span>
                                <span class="field-value" js-unit-field></span>
                            </div>
                            <div class="details">
                                <span class="field_label">Po #:</span>
                                <span class="field-value" js-po-field></span>
                            </div>
                            <div class="details">
                                <span class="field_label">price:</span>
                                <span class="field-value" js-price-field></span>
                            </div>
                        </div>
                        <details class="frame-details hide__desktop">
                            <summary class="view-details">
                                <span class="label--open">view details</span>
                                <span class="label--close">hide details</span>
                            </summary>
                            <div class="details-panel">
                                <div class="column__grid">
                                    <div class="block__column">
                                        <h4 class="column__title">Mirror dimensions *</h4>
                                        <div class="details__grid">
                                            <div class="details">
                                                <span class="field_label">width:</span>
                                                <span class="field-value" js-width-field></span>
                                            </div>
                                            <div class="details">
                                                <span class="field_label">height:</span>
                                                <span class="field-value" js-height-field></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="block__column">
                                        <h4 class="column__title">Space around mirror *</h4>
                                        <div class="details__grid column--grid">
                                            <div class="details">
                                                <span class="field_label">top:</span>
                                                <span class="field-value" js-clearance-top-field></span>
                                            </div>
                                            <div class="details">
                                                <span class="field_label">bottom:</span>
                                                <span class="field-value" js-clearance-bottom-field></span>
                                            </div>
                                            <div class="details">
                                                <span class="field_label">left:</span>
                                                <span class="field-value" js-clearance-left-field></span>
                                            </div>
                                            <div class="details">
                                                <span class="field_label">right:</span>
                                                <span class="field-value" js-clearance-right-field></span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="block__column">
                                        <h4 class="column__title">clips or channels * </h4>
                                        <div class="details__grid">
                                            <div class="details">
                                                <span class="field_label">position:</span>
                                                <span class="field-value" js-clips-field></span>
                                            </div>
                                            <div class="details">
                                                <span class="field_label">Larger than 1”:</span>
                                                <span class="field-value" js-clip-size-field></span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </details>
                    </div>
                </div>
                <div class="details__grid column--grid hide__mobile">
                    <div class="details">
                        <span class="field_label">Qty:</span>
                        <span class="field-value" js-quantity-field></span>
