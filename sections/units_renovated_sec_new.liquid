{{ 'units_renovated_sec_new.css' | asset_url | stylesheet_tag }}
{%- style -%}
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
    padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
  }
{% if section.settings.bgimg_mobile != blank %}
  .units_renovated_main_wrapper{background:url({{ section.settings.bgimg_mobile | img_url:'master' }}) center no-repeat;background-size:100%;}
 {% endif %}

    @media screen and (min-width: 750px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top }}px;
      padding-bottom: {{ section.settings.padding_bottom }}px;
    }
  {% if section.settings.bgimg != blank %}
  .units_renovated_main_wrapper{background:url({{ section.settings.bgimg | img_url:'master' }}) center no-repeat;background-size:100%;}
 {% endif %}
  }
        @media (max-width: 1100px) and (min-width: 750px) {
     .units_renovated_main_wrapper  {background-size:cover;}  
  .units_renovated_main_wrapper .page-width{padding-right:0!important;}
  .units_renovated_main_wrapper .swiper-scrollbar{right:20px!important;}
    }
{%- endstyle -%}
 {% if section.settings.main_heading != blank or section.blocks.size > 0 %}
<div class="units_renovated_main_wrapper renovated_section section-{{ section.id }}-padding color-{{ section.settings.section_color_scheme }}">
 <div class="page-width">
   {% if section.settings.main_heading != blank %}
   <div class="units_renovated_main_heading Common_title_new">
     <h3>{{ section.settings.main_heading }}</h3>
   </div>
   {% endif %}
{% if section.blocks.size > 0 %}
   <div class="swiper renovated_slider">
     <div class="swiper-wrapper">
   {% for block in section.blocks %}
  <div class="swiper-slide">
    <div class="units_renovated_review_wrapper">
      {% if block.settings.review_image != blank %}
      <div class="units_renovated_review_image">
        <img src="{{ block.settings.review_image | img_url:'master' }}" />
      </div>
        {% endif %}
      <div class="units_renovated_review_content_sec">
        {% if block.settings.review_show == true %}
