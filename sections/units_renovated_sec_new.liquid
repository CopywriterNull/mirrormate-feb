        </div>
          {% endif %}
        {% if block.settings.review_title != blank %}
          <div class="units_renovated_review_title"><h4>{{ block.settings.review_title }}</h4></div>
        {% endif %}
         {% if block.settings.review_description!= blank %}
          <div class="units_renovated_review_description">{{ block.settings.review_description }}</div>
        {% endif %}
{% if block.settings.reviewer_name != blank %}
        <div class="units_renovated_reviewer_name">{{ block.settings.reviewer_name }}</div>
  {% endif %}
        {% if block.settings.reviewer_designation != blank %}
        <div class="units_renovated_reviewer_designation">{{ block.settings.reviewer_designation }}</div>
  {% endif %}
      </div>
    </div>
  </div>
   {% endfor %}
     </div>
     <div class="swiper-scrollbar"></div>
   </div>
  {% endif %}
 </div>
</div>
   {% endif %}
{% schema %}
  {
    "name": "Units Renovated",
    "settings": [
      {
        "type": "image_picker",
        "id": "bgimg",
        "label": "Background Image"
      },
      {
        "type": "image_picker",
        "id": "bgimg_mobile",
        "label": "Background Mobile Image"
      },
    {
      "type": "color_scheme",
      "id": "section_color_scheme",
      "label": "t:sections.all.colors.label",
      "default": "scheme-1"
    },
      {
        "type": "text",
        "id": "main_heading",
        "label": "Main Heading"
      },
     {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 105,
      "step": 5,
      "unit": "px",
      "label": "t:sections.all.padding.padding_top",
      "default": 105
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 105,
      "step": 5,
      "unit": "px",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 105
    }
    ],
    "blocks": [
      {
        "type": "review",
        "name": "Review",
        "settings": [
      {
        "type": "image_picker",
        "id": "review_image",
        "label": "Review Image"
      },
          {
            "type": "checkbox",
            "id": "review_show",
            "label": "Show Rating",
            "default":false
          },
          {
            "type": "text",
            "id": "review_title",
            "label": "Review Heading"
          },
           {
            "type": "richtext",
            "id": "review_description",
            "label": "Review Description"
          },
          {
            "type": "text",
            "id": "reviewer_name",
            "label": "Reviewer Name"
          },
          {
            "type": "text",
            "id": "reviewer_designation",
            "label": "Reviewer Designation"
          }
        ]
      }
    ],
    "presets": [
      {
        "name": "Units Renovated"
      }
    ]
  }
{% endschema %}